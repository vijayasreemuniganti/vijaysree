# list of alphabets
alphabets = ['a', 'b', 'd', 'e', 'i', 'j', 'o']
# function that filters vowels
def filterVowels(alphabet):
  vowels = ['a', 'e', 'i', 'o', 'u']
  if (alphabet in vowels):
    return True
  else:
    return False
filteredVowels = filter(filterVowels, alphabets)
print('The filtered vowels are:')
for vowel in filteredVowels:
   print(vowel)
